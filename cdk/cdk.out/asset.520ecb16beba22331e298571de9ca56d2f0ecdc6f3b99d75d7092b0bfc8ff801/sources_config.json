{
    "sources": [
      {
        "name": "windows-sysmon",
        "preprocessor_module": "sysmon_preprocessor",
        "input_paths": {
          "kinesis": {
            "enabled": true,
            "metadata_field": "source"
          }
        },
        "mapping_file": "windows-sysmon-mapping.json"
      },
      {
        "name": "aws-alb",
        "preprocessor_module": "alb_preprocessor",
        "input_paths": {
          "s3": {
            "enabled": true,
            "source_buckets": [
              {
                "bucket_name": "ocsf-transform-infrastructure-s3-staging-log-bucket", 
                "prefix": "alb-logs/"
              }
            ]
          }
        },
        "mapping_file": "alb-mapping.json"
      }
    ]
  }